generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model apartamento{
  condominioid Int @id @default(autoincrement())
  nomecondominio String
  endereco String
  telefone String
}

model clientes {
  clienteid Int @id @default(autoincrement())
  apartamento String
  nome String
  cpf String @unique
}
